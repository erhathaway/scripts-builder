{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "lib": [
      "es2015",
      "es2016",
      "esnext"
    ],
    "declaration": true,
    "sourceMap": true,
    "outDir": "lib",
    "rootDir": "./src",
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "baseUrl": "./src/",
    "rootDirs": [
      "./src"
    ],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "inlineSources": true
  },
  "exclude": [
    "node_modules",
    "lib",
    "db"
  ],
  "include": [
    "/home/ethan/Documents/test/"
  ]
}